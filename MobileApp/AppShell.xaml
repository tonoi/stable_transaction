<?xml version="1.0" encoding="utf-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:JPYCOffline.Pages"
    x:Class="JPYCOffline.AppShell">

    <ShellContent Route="login" ContentTemplate="{DataTemplate pages:Login}" />
    <ShellContent Route="signup" ContentTemplate="{DataTemplate pages:SignUp}" />
    <ShellContent Route="passcode" ContentTemplate="{DataTemplate pages:Passcode}" />
    <ShellContent Route="wallet" ContentTemplate="{DataTemplate pages:Wallet}" />
    <ShellContent Route="settings" ContentTemplate="{DataTemplate pages:Settings}" />
    <ShellContent Route="biometric" ContentTemplate="{DataTemplate pages:BiometricAuth}" />
    <ShellContent Route="merchant" ContentTemplate="{DataTemplate pages:MerchantPos}" />
    <ShellContent Route="auth-accounts" ContentTemplate="{DataTemplate pages:AuthenticatorAccounts}" />
</Shell>
