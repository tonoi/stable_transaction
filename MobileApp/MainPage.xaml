<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:OfflineGuide"
             xmlns:maui="clr-namespace:Microsoft.AspNetCore.Components.WebView.Maui;assembly=Microsoft.AspNetCore.Components.WebView.Maui"
             x:Class="OfflineGuide.MainPage">

    <maui:BlazorWebView HostPage="wwwroot/index.html">
        <maui:BlazorWebView.RootComponents>
            <maui:RootComponent Selector="#app" ComponentType="{x:Type local:BlazorApp}" />
        </maui:BlazorWebView.RootComponents>
    </maui:BlazorWebView>

</ContentPage> 